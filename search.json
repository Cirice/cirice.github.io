---
---
[
{% for section in site.data.datascience %}
  {% for item in section.datascience %}
    {% assign item_url = item | prepend:"/datascience/" | append:"/" %}
    {% assign p = site.datascience | where:"url", item_url | first %}
    {
      "title": "{{ p.title }}",
      "url": "{{ p.url | prepend: site.baseurl }}"

    },
  {% endfor %}
{% endfor %}

{% for section in site.data.about %}
  {% for item in section.about %}
    {% assign item_url = item | prepend:"/about/" | append:"/" %}
    {% assign p = site.about | where:"url", item_url | first %}
    {
      "title": "{{ p.title }}",
      "url": "{{ p.url | prepend: site.baseurl }}"

    },
  {% endfor %}
{% endfor %}

  {% for post in site.posts %}
  {
  "title": "{{ post.title | escape }}",
  "url": "{{ post.url | prepend: site.baseurl }}"
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
